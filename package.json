{
  "name": "Server",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "postinstall": "dotnet restore server",
    "start": "node run --debug",
    "test": "node run test --env=jsdom",
    "test:coverage": "node run test --coverage --env=jsdom",
    "tslint": "tslint -c tslint.json 'client/**/*.{ts,tsx}'",
    "clean": "node run clean",
    "build": "node run build",
    "build:debug": "node run build --debug",
    "publish": "node run publish"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "client/**/*.{js,ts,jsx,tsx,mjs}"
    ],
    "mapCoverage": true,
    "testMatch": [
      "<rootDir>/client/**/__tests__/**/*.{js,ts,jsx,tsx,mjs}",
      "<rootDir>/client/**/?(*.)(spec|test).{js,ts,jsx,tsx,mjs}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/history": "4.6.2",
    "@types/jest": "^23.1.4",
    "@types/react": "16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-redux": "6.0.3",
    "@types/react-router": "4.0.28",
    "@types/react-router-dom": "4.2.7",
    "@types/react-router-redux": "5.0.15",
    "@types/webpack": "4.4.5",
    "@types/webpack-env": "1.13.6",
    "aspnet-prerendering": "^3.0.1",
    "aspnet-webpack": "^3.0.0",
    "awesome-typescript-loader": "5.2.0",
    "bootstrap": "3.3.7",
    "browser-sync": "^2.23.6",
    "css-loader": "1.0.0",
    "domain-task": "^3.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "event-source-polyfill": "0.0.12",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "1.1.11",
    "history": "4.7.2",
    "jest": "^23.3.0",
    "jest-cli": "^23.3.0",
    "jquery": "3.3.1",
    "node-noop": "1.0.0",
    "promise": "^8.0.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "16.4.1",
    "react-dev-utils": "^5.0.0",
    "react-dom": "16.4.1",
    "react-hot-loader": "4.3.3",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "style-loader": "0.21.0",
    "ts-jest": "^23.0.0",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "2.9.2",
    "url-loader": "1.0.1",
    "webpack": "4.15.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "2.22.2",
    "webpack-merge": "4.1.3"
  },
  "dependencies": {
    "npm": "^6.1.0"
  }
}
