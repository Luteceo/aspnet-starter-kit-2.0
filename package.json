{
  "name": "Server",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "postinstall": "dotnet restore server",
    "start": "node run",
    "test": "node run test --env=jsdom",
    "tslint": "tslint client/*.ts",
    "build": "node run build",
    "build:debug": "node run build --debug"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "client/**/*.{js,ts,jsx,tsx,mjs}"
    ],
    "mapCoverage": true,
    "testMatch": [
      "<rootDir>/client/**/__tests__/**/*.{js,ts,jsx,tsx,mjs}",
      "<rootDir>/client/**/?(*.)(spec|test).{js,ts,jsx,tsx,mjs}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/history": "4.6.2",
    "@types/jest": "^22.1.3",
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "@types/react-hot-loader": "3.0.6",
    "@types/react-redux": "5.0.15",
    "@types/react-router": "4.0.22",
    "@types/react-router-dom": "4.2.4",
    "@types/react-router-redux": "5.0.12",
    "@types/webpack": "3.8.8",
    "@types/webpack-env": "1.13.5",
    "aspnet-prerendering": "^3.0.1",
    "aspnet-webpack": "^2.0.3",
    "aspnet-webpack-react": "^3.0.0",
    "awesome-typescript-loader": "3.4.1",
    "babel-core": "^6.0.0 || ^7.0.0-0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.1.0",
    "bootstrap": "3.3.7",
    "browser-sync": "^2.23.6",
    "cpy": "^6.0.0",
    "css-loader": "0.28.9",
    "domain-task": "^3.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.6.1",
    "eslint-watch": "^3.1.3",
    "event-source-polyfill": "0.0.12",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "history": "4.7.2",
    "jest": "^22.1.4",
    "jest-cli": "^22.1.4",
    "jquery": "3.3.1",
    "node-noop": "1.0.0",
    "npm-run-all": "^4.1.2",
    "object-assign": "^4.1.1",
    "popper.js": "^1.12.9",
    "promise": "^8.0.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "16.2.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "3.1.3",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "style-loader": "0.20.1",
    "ts-jest": "^22.0.4",
    "tslint": "^5.9.1",
    "typescript": "2.7.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "2.21.0",
    "webpack-merge": "4.1.1",
    "whatwg-fetch": "^2.0.3"
  }
}
